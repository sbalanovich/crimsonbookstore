 * styles.css
 *
 * Crimson Bookstore
 * Lauren Urke and Sergeui Balanovich
 *
 * Global CSS.
 ***************************************************************************/
@import "mixins";

head {
  margin: 0px;
}

body{
  margin: 0px;
  background-color: #7D0000; 
  overflow:auto;
}

.navbar{
  display: block;
  margin-bottom: 20px;
  overflow: visible;
  color: #777777;
  .brand {
    text-shadow: 0px 2px 1px #222;
    color: rgba(255, 255, 255, 0.75);
    &:hover {
      text-shadow: 0px -1px 2px #222;
    }

    &:active {
      text-shadow: 0px -1px 2px #222;
    }
  }
  .navbar-inner {
    .nav {
      li{
        a{
          text-shadow: 0px 2px 1px #222;
          color: rgba(255, 255, 255, 0.75);
          &:hover {
            text-shadow: 0px -1px 2px #222;
          }

          &:active {
            text-shadow: 0px -1px 2px #222;
          }
        }
      }
      li.dropdown.open {
          .dropdown-toggle {
            background-color: rgba(255, 255, 255, 0);
            text-shadow: 0px -2px 0px #222;
            color: rgba(255, 255, 255, 0.75);

          }
      }
      ul{
        background-image: url("../img/lightgrey.jpg");
        @include box-shadow(0 1px 2px rgba(0, 0, 0, 0.4), inset 0 1px rgba(255, 255, 255, 0.34));
        border: none;
        li {
          margin: 5px;
        }
      }
    }
  }
}


#mainpage {

}

.secondnav {

}

/* pill bar*/
#pill-bar {
  font-weight: bold;
  width: auto;
  
  #buy {
    margin-left: 2%;
    width: 48%;
  }
  
  #sell {
    margin-right: 2%;
    width: 48%;
  }
  ul {
    padding: 0;
    margin: 0 0 10px 0px;
  }
  ul li {
    display: inline-block;
  }
  ul li a {
    display: block;
    text-align: center;
  }
  .inactive a {
    color: #222;
    text-shadow: 0 1px 1px white;
    background: -webkit-gradient(linear, left top, left bottom, from(#eee), to(#ddd));
    background: -moz-linear-gradient(top center, #eee, #ddd);
    -webkit-box-shadow: inset 0 0 5px white;
    -moz-box-shadow: inset 0 0 5px white;
    padding: 20px 0;
    -webkit-box-shadow: inset 0 0 5px white;
    -moz-box-shadow: inset 0 0 5px white;
    border: 1px solid #ccc;
    z-index: 1;
    &:hover {
      background: -webkit-gradient(linear, left bottom, left top, from(#eee), to(#ddd));
      background: -moz-linear-gradient(top center, #ddd, #eee);
    }
    &:focus {
      outline: none;
    }
  }
  .active a{
    padding: 28px 0 20px 0;
    border: 1px solid #666666;
    background: -webkit-gradient(linear, left top, left bottom, from(#003366), to(#0066CC));
    background: -moz-linear-gradient(top center, #003366,#0066CC);
    text-shadow: 0 1px 1px black;
    -webkit-border-radius: 10px 10px 0 0;
    -webkit-background-clip: padding-box;
    -webkit-box-shadow: inset 0 0 1px #fd0000;
    -moz-border-radius: 10px 10px 0 0;
    -moz-background-clip: padding-box;
    -moz-box-shadow: inset 0 0 1px #fd0000;
    z-index: 2;
    color: white;
    text-shadow: 0 1px 1px black;
    &:focus {
      outline: none;
    }
  }
}
 /* Style of pill Content */
#pill-content {
  padding: 0px 25px 30px 25px;
  line-height: 20px;
  margin-left: 0px;
}

#centercontent
{
  background: #FFFFFF; 
  overflow:auto; 
  padding-left:30px; 
  padding-right:30px;
  padding-bottom:30px;
  @include box-shadow(0 3px 12px rgba(0, 0, 0, 0.7), inset 0 1px rgba(255, 255, 255, 0.34));
}

#sellsearchbar{

}

#message
{
  background: #FFFFFF; 
  overflow:auto; 
  padding:1px;
  @include box-shadow(0 3px 12px rgba(0, 0, 0, 0.7), inset 0 1px rgba(255, 255, 255, 0.34));
}

#sellsearchresults {

}

#sellformdiv {
  #sellformpreset {

  }
  #sellform {

  }
}


#bottom {
  font-size: smaller;
  margin: 20px;
  text-align: center;
  color:#000000; padding:15px;
}

#welcomeuser
{
  padding:15px;
  color:#FFFFFF;
}

::-webkit-scrollbar {
    width: 10px;
}
 
/* Track */
::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px #A00000; 
    -webkit-border-radius: 10px;
    border-radius: 10px;
}
 
/* Handle */
::-webkit-scrollbar-thumb {
    -webkit-border-radius: 10px;
    border-radius: 10px;
    background: #A00000; 
    -webkit-box-shadow: inset 0 0 6px #A00000; 
}
::-webkit-scrollbar-thumb:window-inactive {
	background: #A00000; 
}

table 
{
  font: 11px/24px Verdana, Arial, Helvetica, sans-serif;
  border-collapse: collapse;
  width: 1700px;
  margin: auto;
}

th 
{
  padding: 0 0.5em;
  text-align: center;
  th {
    border-left: 1px solid #CCC;
    text-align: center;
  }
}

tr.yellow td 
{
  border-top: 1px white;
  border-bottom: 1px white;
  background: #FFC;
}

td 
{
  border-bottom: 1px white;
  padding: 0 0.5em;
  width:100px;
  td {
    border-left: 1px solid #CCC;
    text-align: center;
  }
}

