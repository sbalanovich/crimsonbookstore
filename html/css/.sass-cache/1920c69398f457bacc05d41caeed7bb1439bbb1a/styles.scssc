.arrow_box {
    position: relative;
    background: #731515;
    border: 4px solid #c2e1f5;
} 
.arrow_box:after, .arrow_box:before {
    top: 100%;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
}

.arrow_box:after {
    border-color: rgba(115, 21, 21, 0);
    border-top-color: #731515;
    border-width: 30px;
    left: 50%;
    margin-left: -30px;
}
.arrow_box:before {
    border-color: rgba(194, 225, 245, 0);
    border-top-color: #c2e1f5;
    border-width: 36px;
    left: 50%;
    margin-left: -36px;
} 

.container
{
    margin-left: auto;
    margin-right: auto;
    text-align: center;
}
#top
{
    // center contents 
    text-align: center;
}

#middle
{
     //center contents 
    text-align: center;
}
#navbtn
{
    width:50px;
    height:20px;
    transition: width 1s, height 1s, transform 1s;
    -moz-transition: width 2s, height 2s, -moz-transform 2s;
    -webkit-transition: width 2s, height 2s, -webkit-transform 2s;
    -o-transition: width 2s, height 2s,-o-transform 2s;
}

#navbtn:hover
{
    width:100px;
    height:20px;
} 

#loginheader
{
font-family:"Times New Roman", Times, serif;
color:#F0F7FF;
}

.background {
    background: none repeat scroll 0 0 #FFFFFF;
    height: 200%;
    left: -50%;
    position: fixed;
    width: 200%;}

.background img{
    bottom: 0;
    display: block;
    left: 0;
    margin: auto;
    min-height: 50%;
    min-width: 50%;
    right: 0;
    top: 0;}

#departnav {
    max-height: 500px;
    overflow: hidden;
    overflow-y: auto;
}


#departnavbtn {
  background-image: -moz-linear-gradient(top, #800000, #400000);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#800000), to(#400000));
  background-image: -webkit-linear-gradient(top, #800000, #400000);
