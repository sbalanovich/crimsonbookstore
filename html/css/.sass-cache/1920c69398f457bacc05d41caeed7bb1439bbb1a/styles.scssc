      text-shadow: 0 1px 1px rgba(0, 0, 0, 0.75);
      @include box-shadow(inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 1px 3px rgba(0, 0, 0, 0.5));
    }
  }
}

#bottom {
  font-size: smaller;
  margin: 20px;
  margin-top: 0px;
  margin-bottom:0px;
  text-align: center;
  color:#000000; 
  padding:15px;
}

#welcomeuser
{
  padding:15px;
  color:#FFFFFF;
}

::-webkit-scrollbar {
    width: 10px;
}
 
/* Track */
::-webkit-scrollbar-track {
    //-webkit-box-shadow: inset 0 0 6px #A00000; 
    //-webkit-border-radius: 10px;
    //border-radius: 10px;
    background-color: rgba(64, 34, 23, 0.93);
}
 
/* Handle */
::-webkit-scrollbar-thumb {
    -webkit-border-radius: 10px;
    border-radius: 10px;
    background: rgb(111, 1, 1); 
    //-webkit-box-shadow: inset 0 0 6px #A00000; 
}
::-webkit-scrollbar-thumb:window-inactive {
	background: #ffffff; 
}

th 
{
  padding: 0 0.5em;
  text-align: center;
  th {
    border-left: 1px solid #CCC;
    text-align: center;
  }
}

tr.yellow td 
{
  border-top: 1px white;
  border-bottom: 1px white;
  background: #FFC;
}

td 
{
  border-bottom: 1px white;
  padding: 0 0.5em;
  width:100px;
  td {
    text-align: center;
  }
