#navbtn
{
    width:50px;
    height:20px;
    transition: width 1s, height 1s, transform 1s;
    -moz-transition: width 2s, height 2s, -moz-transform 2s;
    -webkit-transition: width 2s, height 2s, -webkit-transform 2s;
    -o-transition: width 2s, height 2s,-o-transform 2s;
}

#navbtn:hover
{
    width:100px;
    height:20px;
} 

#loginheader
{
font-family:"Times New Roman", Times, serif;
color:#F0F7FF;
}

.background {
    background: none repeat scroll 0 0 #FFFFFF;
    height: 200%;
    left: -50%;
    position: fixed;
    width: 200%;}

.background img{
    bottom: 0;
    display: block;
    left: 0;
    margin: auto;
    min-height: 50%;
    min-width: 50%;
    right: 0;
    top: 0;}

#departnav {
    max-height: 500px;
    overflow: hidden;
    overflow-y: auto;
}


#departnavbtn {
  background-image: -moz-linear-gradient(top, #800000, #400000);
  background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#800000), to(#400000));
  background-image: -webkit-linear-gradient(top, #800000, #400000);
  background-image: -o-linear-gradient(top, #800000, #400000);
  background-image: linear-gradient(to bottom, #800000, #400000);
  color:#FFFFFF;
}

.centercontent
{
background: #FFFFFF; 
overflow:auto; 
padding-left:30px; 
padding-right:30px;
padding-bottom:30px;
 -moz-box-shadow:    3px 3px 5px 6px #000000;
  -webkit-box-shadow: 0px 0px 5px 6px #000000;
  box-shadow:         -1px 6px 5px 6px #000000;
}

.centercontentsmall
{
background: #FFFFFF; 
overflow:auto; 
padding:10px;
 -moz-box-shadow:    3px 3px 5px 6px #000000;
  -webkit-box-shadow: 0px 0px 5px 6px #000000;
  box-shadow:         -1px 6px 5px 6px #000000;
}

.shadow
{
background: #FFFFFF; 
 -moz-box-shadow:    3px 3px 5px 6px #000000;
  -webkit-box-shadow: 0px 0px 5px 6px #000000;
  box-shadow:         -1px 6px 5px 6px #000000;
}

#coverfloat
{
    padding: 15px;
    height: 250px;
    width: 200px;
    background:url(http://mameara.com/wp-content/uploads/2010/07/Dark-Wood-Texture.jpg); 
    -moz-box-shadow:    3px 3px 5px 6px #000000;
    -webkit-box-shadow: 0px 0px 5px 6px #000000;
    box-shadow:         -1px 6px 5px 6px #000000;
    transition: box-shadow 1s, width 1s, height 1s, transform 1s;
    transition-delay: 0.5s;
    -moz-transition: box-shadow 1s, width 2s, height 2s, -moz-transform 2s;
    -moz-transition-delay:0.5s;
    -webkit-transition: box-shadow 1s, width 2s, height 2s, -webkit-transform 2s;
    -webkit-transition-delay:0.5s;
    -o-transition: box-shadow 1s, width 2s, height 2s,-o-transform 2s;
    -o-transition-delay:0.5;
}
#coverfloat:hover
{
    padding: 15px;
    height: 300px;
    width: 250px;
    background:url(http://mameara.com/wp-content/uploads/2010/07/Dark-Wood-Texture.jpg); 
     -moz-box-shadow:    3px 3px 5px 6px #000000;
    -webkit-box-shadow: 0px 0px 5px 6px #000000;
    box-shadow:         0 8px 5px 10px #000000;

}

#coverpreview
{
    height: 100px;
    width: 80px;
    background: #FFFFFF;    transition: width 1s, height 1s, transform 1s;
    transition-delay: 0.5s;
    -moz-transition:  width 2s, height 2s, -moz-transform 2s;
    -moz-transition-delay:0.5s;
    -webkit-transition:  width 2s, height 2s, -webkit-transform 2s;
    -webkit-transition-delay:0.5s;
    -o-transition:  width 2s, height 2s,-o-transform 2s;
    -o-transition-delay:0.5;
}
#coverpreview:hover
{
    height: 120px;
    width: 100px;
}

