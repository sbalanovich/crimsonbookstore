.arrow_box {
    position: relative;
    background: #731515;
    border: 4px solid #c2e1f5;
} 
.arrow_box:after, .arrow_box:before {
    top: 100%;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
}

.arrow_box:after {
    border-color: rgba(115, 21, 21, 0);
    border-top-color: #731515;
    border-width: 30px;
    left: 50%;
    margin-left: -30px;
}
.arrow_box:before {
    border-color: rgba(194, 225, 245, 0);
    border-top-color: #c2e1f5;
    border-width: 36px;
    left: 50%;
    margin-left: -36px;
} 

.container
{
    margin-left: auto;
    margin-right: auto;
    text-align: center;
}
#top
{
    // center contents 
    text-align: center;
}

#middle
{
     //center contents 
