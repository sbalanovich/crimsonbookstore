::-webkit-scrollbar-thumb {
    -webkit-border-radius: 10px;
    border-radius: 10px;
    background: #b2b2b2; 
    //-webkit-box-shadow: inset 0 0 6px #A00000; 
}
::-webkit-scrollbar-thumb:window-inactive {
	background: #ffffff; 
}

